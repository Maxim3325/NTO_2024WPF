<Page x:Class="NTO_2024WPF.Pages.EducationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NTO_2024WPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EducationPage">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Label Content="Образовательная деятельность"/>
        <Button x:Name="StudiosBtn" Content="Студии" Click="StudiosBtn_Click" Margin="5" HorizontalContentAlignment="Center" Width="100"/>
        <Button x:Name="TeacherBtn" Content="Преподаватели" Margin="5" Click="TeacherBtn_Click" HorizontalContentAlignment="Center" Width="100"/>
    </StackPanel>
</Page>
